{
  "compilerOptions": {
    "target": "es2019", // for Object.fromEntries()
    "module": "esnext",
    "jsx": "react-jsx", // or "react-jsxdev"
    "jsxImportSource": "preact",
    "strict": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
